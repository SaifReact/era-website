INSERT INTO `testimonials` VALUES
(1,'https://www.nerdsharbor.com/cms_assets/front_page/testimonials/Standard-Bank-100x100.jpg','Standard Bank Limited','Mr. Md. Nazmus Salehin','Managing Director & CEO',"We wish to compliment one of the best Software Company ERA-InfoTech Ltd. for continuously providing us quality Software Solution like Core Banking Solution (CBS), Anti Money Laundering, Cheque processing, HR & Payroll Management Solution etc for the Automation of our Bank. Their consistent support towards solving our problem will definitely enhance the relationship between these two prestigious organization. We are happy to have ERA as our Technology Partner. We wish ERA's success in its future journey.",1,'2021-09-19 11:30:08','2021-09-19 11:38:56'),
(2,'https://www.nerdsharbor.com/cms_assets/front_page/testimonials/Sadek-md-sohel-100x100.png','BCBL','Abu Sadek Md. Sohel','Managing Director and CEO','BCBL is using Core Banking Solution (CBS), Anti Money Laundering, Cheque processing, HR & Payroll Management Solution developed by ERA-InfoTech Ltd., we are able to run our daily business Operation without any worries. We believe, ERA-InfoTech Ltd. has enough strength to acquaint State of the Art Technology for Financial Industries. We wish ERA a happy, successful, challenging and exciting future.',2,'2021-09-19 11:31:14','2021-09-19 11:39:07'),
(3,'https://www.nerdsharbor.com/cms_assets/front_page/testimonials/Sadek-md-sohel-100x100.png','BCBL','Abu Sadek Md. Sohel','Managing Director and CEO','BCBL is using Core Banking Solution (CBS), Anti Money Laundering, Cheque processing, HR & Payroll Management Solution developed by ERA-InfoTech Ltd., we are able to run our daily business Operation without any worries. We believe, ERA-InfoTech Ltd. has enough strength to acquaint State of the Art Technology for Financial Industries. We wish ERA a happy, successful, challenging and exciting future.',2,'2021-09-19 11:32:14','2021-09-19 11:40:07');

INSERT INTO `audits` VALUES
(1,'App\\Models\\User',1,'created','App\\Models\\Event',1,'[]','{\"title\":\"Agreement Signing with CR2\",\"slug\":\"agreement-signing-with-cr2\",\"meta\":null,\"thumbnail\":\"https:\\/\\/www.nerdsharbor.com\\/cms_assets\\/events\\/event_1\\/image1.jpg\",\"image\":\"https:\\/\\/www.nerdsharbor.com\\/cms_assets\\/events\\/event_1\\/image1.jpg\",\"event_at\":\"2019-04-20\",\"teaser\":\"An Agreement Signing Ceremony between ERA-InfoTech Ltd and CR2 Ltd was held to provide sophisticated software services to banks on 20 April 2019 in presence of Mr. A. Rouf Chowdhury\",\"detail\":\"<p>An Agreement Signing Ceremony between ERA-InfoTech Ltd and CR2 Ltd was held to provide sophisticated software services to banks on 20 April 2019 in presence of Mr. A. Rouf Chowdhury, Chairman of ERA-InfoTech Ltd and Bank Asia Ltd in the Capital City. Mr. Fintan Byrne, Chief Executive Officer, CR2 and Md. Serajul Islam, FCMA, CEO of ERA-Infotech Ltd. signed on the agreement on behalf of their respective organizations.<\\/p>\\n<p>Mr. Nafees Khundker, Director of ERA-InfoTech Ltd, Mr. Enam Chowdhury, Director, Mr. Mohammad Borhanuddin, President &amp; Managing Director (CC), Mr. Sarder Akhter Hamed, Head of Channel Banking, Mr. Hossain Ahmad, CIO, Mr. Md. Saiful Islam, CTO from Bank Asia Ltd, Mr. Andrew Chisembele, SVP, Sales from CR2 and Mr. Tauhidul Hoque, CTO along with other high officials of ERA-Infotech were present in the program.<\\/p>\\n<p>CR2 Ltd, an Irish company, proudly serves over 100 banks in 60 countries including ANZ &amp; SCB, providing value and making banking simple. They provide innovative omnichannel banking software to banks all around the world, from ATM to mobile and POS to internet, they make self-service banking better.<\\/p>\\n<p>ERA-InfoTech Ltd, the pioneer in automation of financial inclusion and software development in Bangladesh will market the switching software jointly with CR2 in this region as a technology partner.<\\/p>\\n<div class=\\\"row\\\">\\n<div cla